{
  "name": "llmr-gl",
  "description": "A WebGL interactive maps library",
  "version": "0.0.1",
  "main": "js/llmr.js",
  "dependencies": {
    "geojson-rewind": "~0.1.0",
    "rbush": "~1.3.2",
    "pbf": "0.0.1",
    "glify": "~0.1.0",
    "unitbezier": "0.0.0"
  },
  "devDependencies": {
    "browserify": "~3.24.11",
    "spritesmith": "~0.17.4",
    "uglify-js": "~2.4.12",
    "grunt": "~0.4.2",
    "grunt-cli": "~0.1.13",
    "grunt-browserify": "~1.3.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-contrib-jshint": "~0.8.0",
    "grunt-contrib-uglify": "~0.3.2",
    "beefy": "https://github.com/chrisdickinson/beefy/archive/beefy-two.tar.gz",
    "glify": "0.0.0",
    "tape": "~2.12.1",
    "watchify": "~0.6.3"
  },
  "browserify": {
    "transform": [
      "glify"
    ]
  },
  "scripts": {
    "test": "tape test/*.js",
    "start": "beefy -t glify debug/site.js -d",
    "prepublish": "grunt"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest"
    ]
  }
}
